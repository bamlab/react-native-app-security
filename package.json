  "homepage": "https://github.com/bamlab/react-native-app-security#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/react": "^18.0.25",
    "expo-module-scripts": "^3.0.11",
    "expo-modules-core": "^1.5.11",
    "release-it": "^18.1.2"
  },
  "packageManager": "yarn@1.22.21",
